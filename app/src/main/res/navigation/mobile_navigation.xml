<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_auth">

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.example.opagg.ui.map.MapFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_map" />

    <fragment
        android:id="@+id/navigation_addnew"
        android:name="com.example.opagg.ui.addnew.AddnewFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_addnew" />


    <fragment
        android:id="@+id/navigation_auth"
        android:name="com.example.opagg.ui.auth.PhoneAuthFragment"
        android:label="@string/title_auth"
        tools:layout="@layout/fragment_phone_auth" />
</navigation>